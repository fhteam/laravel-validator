{
  "name": "fhteam/laravel-validator",
  "description": "A bunch of Laravel validators",
  "minimum-stability": "stable",
  "license": "GPL-2.0-only",
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "laravel/framework": "~5.0",
    "squizlabs/php_codesniffer": "1.*",
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "4.3.*",
    "satooshi/php-coveralls": "dev-master",
    "giggsey/libphonenumber-for-php": "~7.0"
  },
  "suggest": {
    "giggsey/libphonenumber-for-php": "Allows to validate phone numbers"
  },
  "autoload": {
    "psr-4": {
      "FHTeam\\LaravelValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FHTeam\\LaravelValidator\\Tests\\Fixture\\": "tests/fixture",
      "FHTeam\\LaravelValidator\\Tests\\": "tests/src"
    }
  }
}
